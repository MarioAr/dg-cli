{
  "name": "cli",
  "version": "1.0.0",
  "main": "app/mauro.js",
  "license": "MIT",
  "scripts": {
    "start": "node app/mauro.js",
    "compile": "pkg app/mauro.js --out-path dist --targets node16-linux-arm64"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "inquirer": "^9.2.12"
  },
  "pkg": {
    "scripts": "app/**/*.js",
    "assets": "app/**/*",
    "targets": [
      "node16-linux-arm64"
    ],
    "outputPath": "dist"
  }
}
